        <cuboid min="27,15,32" max="25,99,29"/>
        <cuboid min="26,13,32" max="25,99,29"/>
        <cuboid min="29,16,8" max="25,99,29"/>
        <cuboid min="28,14,-0" max="25,99,29"/>
        <cuboid min="27,13,-5" max="25,99,29"/>
        <cuboid min="26,12,-8" max="25,99,29"/>
