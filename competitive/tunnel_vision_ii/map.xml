        <cuboid min="66,14,32" max="68,99,29"/>
        <cuboid min="65,16,32" max="68,99,29"/>
        <cuboid min="64,16,31" max="68,99,29"/>
        <cuboid min="63,17,24" max="68,99,29"/>
